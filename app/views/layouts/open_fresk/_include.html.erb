<%= stylesheet_link_tag "application", media: "all", "data-turbo-track": "reload" %>
<%= csrf_meta_tags %>

<%= favicon_link_tag "favicon.png", type: "image/png" %>
<%= favicon_link_tag "favicon.png", rel: "apple-touch-icon", type: "image/png" %>

<link
  rel="stylesheet"
  href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/cupertino/jquery-ui.min.css"
  />

<link rel="manifest" href="<%= asset_path 'manifest.json' %>" />

<%= javascript_importmap_tags %>

<%= javascript_include_tag "open_fresk",
                           "data-turbo-track": "reload",
                           defer: true %>

<script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
<script src="https://code.jquery.com/ui/1.11.2/jquery-ui.min.js"></script>

<link
  href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,400;0,700;1,100;1,400;1,700&display=swap"
  rel="stylesheet"
  />

<%#— hook for host-only extras —%>
<% if lookup_context.template_exists?("layouts/open_fresk/include_extras", [], true) %>
  <%= render "layouts/open_fresk/include_extras" %>
<% end %>